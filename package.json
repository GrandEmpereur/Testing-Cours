{
  "name": "testing-cours",
  "version": "1.0.0",
  "main": "./build/server.js",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --files ./src/server.ts",
    "build": "rimraf ./build && yarn tsc",
    "start": "node --experimental-specifier-resolution=node ./build/server.js",
    "tsc": "tsc",
    "prepare": "if [ -z \\\"$CI\\\" ]; then husky install; fi",
    "postinstall": "yarn build",
    "test": "vitest run --coverage",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "cross-env": "^7.0.3",
    "faker-br": "^0.4.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "node-env": "^0.1.6",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@types/koa": "^2.15.0",
    "@types/koa-router": "^7.4.8",
    "@types/koa2-ratelimit": "^0.9.6",
    "@types/koa__cors": "^5.0.0",
    "@types/node": "^20.12.12",
    "@vitest/ui": "^1.6.0",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "get-port": "^7.1.0",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.1",
    "koa2-ratelimit": "^1.1.3",
    "supertest": "^7.0.0"
  }
}