{
  "name": "testing-cours",
  "version": "1.0.0",
  "main": "./build/server.js",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --files ./src/server.ts",
    "build": "rimraf ./build && yarn tsc",
    "start": "node --experimental-specifier-resolution=node ./build/server.js",
    "tsc": "tsc",
    "prepare": "if [ -z \\\"$CI\\\" ]; then husky install; fi",
    "postinstall": "yarn build",
    "test": "vitest run --coverage",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "get-port": "^7.1.0",
    "node-env": "^0.1.6",
    "rimraf": "^3.0.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa2-ratelimit": "^0.9.3",
    "@types/koa__cors": "^3.1.1",
    "@types/node": "^20.12.12",
    "@vitest/coverage-v8": "^1.6.0",
    "axios": "^1.6.8",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.1",
    "jest": "^29.7.0",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "koa2-ratelimit": "^1.1.3",
    "vite": "^5.2.11"
  }
}
